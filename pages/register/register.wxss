.introbgc{
  width: 100%;
  height: 270rpx;
  position: relative;
}
.introbgc image{
  width: 100%;
  height: 100%;
}
.introbgc text.tit{
  width: 100%;
  height: 120rpx;
  font-size: 34rpx;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  top:0;
  left:0;
}
.fabubox{
  width: 94%;
  position: absolute;
  top:120rpx;
  left: 3%;
  margin-bottom: 200rpx;
  
}
.fabubox .fabu{
  width: 90%;
  padding:0 5% 20rpx 5%;
  box-shadow: 0 0 20rpx #f6f6f6;
  background: #fff;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
}
.fabubox .fabu .tit{
  width: 100%;
  font-size: 28rpx;
  color: #000;
  line-height:95rpx;
  font-weight: bold;
  position: relative;
}
.cpbox{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  overflow: hidden;
}
.cpbox text{
  display: inline-block;
  background:#fff;
  font-size:24rpx;
  color:#000;
  height:50rpx;
  line-height:50rpx;
  border-radius:25rpx;
  padding: 0 20rpx;
  margin: 0 20rpx 20rpx 0;
  background: #f6f6f6;
}
.cpbox text.active{
background:#007a33;
color:#fff;
}
.fabubox .fabu .inputbox{
  width: 100%;
  height: 92rpx;
  border: 1px solid #f0f0f0;
  border-radius: 4rpx;
  position: relative;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
}
.fabubox .fabu .inputbox .saiqu{
  width: 100%;
  height: 92rpx;
}
.fabubox .fabu .inputbox image.chechicon{
  width: 23rpx;
  height: 13rpx;
  position: absolute;
  top:40rpx;
  right: 4%;
}
.inputbox .sexbox{
  display: flex;
  flex-wrap: wrap;
  margin-right: 4%;
}
.inputbox .sexbox view{
display: flex;
  flex-wrap: wrap;
  margin-left: 30rpx;
  align-items: center;
}
.inputbox .sexbox view image{
  width: 21rpx;
  height: 21rpx;
  margin-right: 17rpx;
}
.inputbox .sexbox view text{
  font-size: 26rpx;
  color: #707070;
}
.fabubox .fabu .inputbox input,.fabubox .fabu .inputbox .saiqu view{
  width:92%;
  padding: 0 4%;
  height: 92rpx;
  line-height: 92rpx;
  font-size: 24rpx;
  color: #000;
}
.fabubox .fabu .inputbox input.small1{
  width:50%;
}
.fabubox .fabu .inputbox input.small2{
  width:80%;
}
.fabubox .tl{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.fabubox .tl .check{
  width: 28rpx;
  height: 28rpx;
  margin:0 35rpx 0 4%;
}
.fabubox .tl text.tt{
font-size: 24rpx;
  color:#6B6B6B;
}
.fabubox .btn {
  display: block;
  width: 90%;
  background: #F11A1A;
  background:#007a33;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  height: 92rpx;
  line-height: 92rpx;
  border-radius: 46rpx;
  margin:0 auto 50rpx auto;
}
.fabubox .btn button{
  background: none;
  border:none;
  color: #fff;
}