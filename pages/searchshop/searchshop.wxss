.input {
  width:92%;
  background: #fff;
  height: 96rpx;
  margin-bottom: 1px;
  position: fixed;
  top:0;
  left:4%;
  z-index: 100;
  
}
.input .allcity{
  position: absolute;
  width: 86%;
  padding: 0 7%;
  background: #fff;
  top:100rpx;
  left: 0;
}
.input .bindsousuo{
  width: 100%;
  line-height: 80rpx;
  color: #333333;
  font-size: 24rpx;
}
.input .sousuo{
  width: 92%;
  height: 96rpx;
  margin: auto;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  background: #fff;
  
}
.input .sousuo image.dizhi{
  width: 30rpx;
  height: 32rpx;
  margin-right: 10rpx;
}
.input .sousuo .inputbox{
  width:73%;
  height: 64rpx;
}
.input .sousuo .inputbox input {
  width:100%;
  height: 64rpx;
  border-radius: 40rpx;
  font-size: small;
  box-sizing: border-box;
}
.input .sousuo text{
  display: inline-block;
  padding: 0 30rpx;
  height: 48rpx;
  line-height: 48rpx;
  border-radius: 24rpx;
  background:#007a33;
  color: #fff;
  font-size: 28rpx;
  position: absolute;
  right:0;
  top:24rpx;

}
