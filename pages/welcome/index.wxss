
.top {
  background: #fff;
  text-align: center;
  overflow:hidden;

}

.logo {
  width: 600rpx;
  height: 152rpx;
  margin: 120rpx auto 35rpx auto;
  padding-bottom: 70rpx;
  text-align: center;
  border-bottom: 1px solid #e0e0e0;
}
.logo image{
  width: 152rpx;
  height: 152rpx;
}

.info {
  padding-bottom: 40rpx;
}

.title {
  color: #333;
  font-weight: bold;
  font-size: 36rpx;
}

.desc {
  color: #999;
  font-size: 28rpx;
  margin-top: 20rpx;
}

.btn {
  background: #007a33;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  border-radius: 50rpx;
  width: 80%;
  margin: 55rpx auto 0;
}
.login_acreen{ 
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index:5;  
  background:rgba(0,0,0,.5);
  }
.login_model{
  position: absolute;
  top: 36vh;
  z-index: 100;
  background: #fff;
  color: #888;
  width: 80%;
  margin-left: 10%;
  border-radius:10rpx; 
  text-align: center;
  box-shadow: 0 0 10rpx #999;
}
.login_model_title{
  margin: 40rpx 0;
  color: #060407;
}
.login_model_body{
  margin: 0 30rpx 30rpx 30rpx;
  font-size: 35rpx;
  line-height: 35rpx;
  color: #a6a4a7;
}
.login_model_body view{
  margin: 15rpx 0;
}
.login_model_btn{
  color: #5fe745;
  letter-spacing: 5px;
} 

